{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./src",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "noEmit": true,
    "resolveJsonModule": true
  },
  "include": ["src", "src/main.tsx", "Archive/GolferProfile.tsx"]
}
