{
  "version": 2,
  "builds": [
    {
      "src": "golf-club-ui/package.json",
      "use": "@vercel/static-build",
      "config": { 
        "distDir": "dist",
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
