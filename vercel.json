{
  "version": 2,
  "buildCommand": "cd golf-club-ui && npm run build",
  "outputDirectory": "golf-club-ui/dist",
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "regions": ["iad1"],
  "alias": "golf-club-ui-lac.vercel.app",
  "env": {
    "VITE_SUPABASE_URL": "@supabase_url",
    "VITE_SUPABASE_ANON_KEY": "@supabase_key",
    "VITE_APP_URL": "https://golf-club-ui-lac.vercel.app",
    "VITE_API_URL": "https://golf-course-recommendation-engin-production.up.railway.app"
  },
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }
      ]
    }
  ]
}
