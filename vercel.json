{
  "version": 2,
  "builds": [
    {
      "src": "golf-club-ui/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "golf-club-ui/build/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}