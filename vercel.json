{
  "version": 2,
  "buildCommand": "cd golf-club-ui && npm install && npm run build",
  "outputDirectory": "golf-club-ui/dist",
  "routes": [
    {
      "src": "^/assets/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/assets/$1"
    },
    {
      "src": "^/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "^/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "^/robots.txt",
      "dest": "/robots.txt"
    },
    {
      "src": "^/golfball.jpg",
      "dest": "/golfball.jpg"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    }
  ]
}