{
  "version": 2,
  "builds": [
    { "src": "server/app.py", "use": "@vercel/python" },
    {
      "src": "golf-club-ui/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "server/app.py" },
    { "src": "/(.*)", "dest": "golf-club-ui/dist/$1" }
  ],
  "cleanUrls": true
}
