{
  "version": 2,
  "builds": [
    {
      "src": "golf-club-ui/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build",
        "cleanUrls": true,
        "basePath": "/golf-club-ui"
      }
    }
  ],
  "routes": [
    {
      "src": "/golf-club-ui/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/golf-club-ui/(.*)",
      "dest": "/golf-club-ui/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/golf-club-ui/build/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}