{
    "builds": [
      { "src": "server/app.py", "use": "@vercel/python" },
      { "src": "golf-club-ui/package.json", "use": "@vercel/static-build" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "server/app.py" },
      { "src": "/(.*)", "dest": "golf-club-ui/build/$1" }
    ]
  }
  